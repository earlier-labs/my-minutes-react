{
  "name": "my-minutes-react",
  "version": "1.0.0",
  "description": "My Minutes built with React, Redux, and TypeScript.",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --content-base public --progress --colors",
    "start:push": "NODE_ENV=development node push-server",
    "test": "NODE_ENV=development ava",
    "build": "NODE_ENV=development webpack --config webpack.dev.js --progress --colors",
    "build:beta": "NODE_ENV=beta webpack --config webpack.beta.js --progress --colors",
    "typings:update": "typings ls | awk '$2 ~ /.+/ {print $2}' | xargs -I {} typings i dt~{} -SG"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ktusznio/my-minutes-react.git"
  },
  "author": "Kamil Tusznio",
  "license": "MIT",
  "homepage": "https://github.com/ktusznio/my-minutes-react",
  "dependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bluebird": "^3.4.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "firebase": "^3.2.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.1",
    "moment": "^2.13.0",
    "moment-duration-format": "^1.3.0",
    "raven-js": "^3.4.1",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "rebass": "^0.2.7",
    "redbox-react": "^1.2.3",
    "redux": "^3.5.2",
    "redux-auth-wrapper": "^0.6.0",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "sw-precache-webpack-plugin": "^0.4.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.2.2",
    "tslint-eslint-rules": "^1.3.0",
    "typescript": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
