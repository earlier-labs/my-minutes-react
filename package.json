{
  "name": "my-minutes-react",
  "version": "1.0.0",
  "description": "My Minutes built with Firebase, React, Redux, and TypeScript.",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --content-base public --progress --colors",
    "test": "NODE_ENV=test ava",
    "build": "NODE_ENV=development webpack --config webpack.dev.js --progress --colors",
    "build:beta": "NODE_ENV=beta webpack --config webpack.beta.js --progress --colors",
    "typings:update": "typings ls | awk '$2 ~ /.+/ {print $2}' | xargs -I {} typings i dt~{} -SG"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ktusznio/my-minutes-react.git"
  },
  "author": "Kamil Tusznio",
  "license": "MIT",
  "homepage": "https://github.com/ktusznio/my-minutes-react",
  "dependencies": {
    "bluebird": "^3.4.6",
    "firebase": "^3.4.1",
    "lodash": "^4.16.2",
    "material-ui": "^0.15.4",
    "moment": "^2.15.1",
    "moment-duration-format": "^1.3.0",
    "raven-js": "^3.7.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^0.8.0",
    "redux-thunk": "^2.1.0",
    "sw-precache-webpack-plugin": "^0.5.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "sw-precache-webpack-plugin": "^0.5.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "tslint-eslint-rules": "^2.1.0",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
